{
  "name": "myna-parser-mnast",
  "version": "0.0.2",
  "description": "Myna parser with Unist compatibility",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench",
    "publish-packages": "pnpm run build && changeset version && changeset publish",
    "unpublish-packages": "pnpm unpublish myna-parser-mnast@0.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "myna",
    "parser",
    "unist",
    "ast",
    "cst"
  ],
  "author": "fustilio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "myna-parser-ts": "workspace:*",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "@changesets/cli": "^2.27.7"
  },
  "dependencies": {
    "unist-builder": "^4.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
