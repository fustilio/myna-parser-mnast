{
  "name": "myna-parser-ts",
  "version": "1.0.0",
  "description": "Myna parser with modern TypeScript support (not published to npm, for local/testing use only)",
  "type": "module",
  "main": "src/index.ts",
  "private": true,
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "myna",
    "parser",
    "unist",
    "ast",
    "cst"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "myna-parser-mnast": "workspace:*",
    "typescript": "^5.3.3",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  }
}
